#lang racket


(define (cube x) (* x x x)

(define (cube-iter guess x)
  (if (good-enough? guess x)
      guess
      (cub-iter (improve guess x)
                 x)))

(define (improve guess x)
  (average guess (/ x guess)))

(define (average x y)
  (/ (+ x y) 2))

(define (good-enough? guess x)
  (< (abs (- (cube guess) x)) 0.001))

(define (cuberoot x)
  (cube-iter 1.0 x))